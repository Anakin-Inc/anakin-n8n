{
  "name": "n8n-nodes-anakin-org",
  "version": "1.1.4",
  "description": "n8n node for Anakin API - web scraping, AI search, and intelligent data extraction",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "scraper",
    "web scraping",
    "ai search",
    "agentic search",
    "perplexity",
    "data extraction",
    "anakin"
  ],
  "license": "MIT",
  "homepage": "https://anakin.io",
  "author": {
    "name": "anakin-organization",
    "email": "technology@anakin.company"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anakin-Inc/anakin-n8n.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AnakinScraperApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AnakinScraper/AnakinScraper.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^5.0.1",
    "n8n-workflow": "*",
    "prettier": "^3.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}

